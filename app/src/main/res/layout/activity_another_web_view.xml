<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_another_web_view_demo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kankan.androidchatwithh5demo.AnotherWebViewActivity">

    <TextView
        android:id="@+id/userBrowser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:padding="10dp"
        android:text="Use Browser"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/entireActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:padding="10dp"
        android:text="Entire Activity"
        android:textSize="20sp" />

</LinearLayout>
